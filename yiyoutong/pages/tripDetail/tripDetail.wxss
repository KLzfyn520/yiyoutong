.container{
    display:flex;
    flex-direction:column;
}
.srcContainer{
    display:flex;
    flex-direction:column;
}
.dstContainer{
    display:flex;
    flex-direction:column;
}
.list{
    display:flex;
    flex-direction:row;
    margin: 0rpx 0rpx 0rpx 20rpx;
    padding: 10rpx 20rpx 10rpx 0rpx;
    align-items: center;
    border-bottom:1px solid #D1D1D2;
}
.listContent{
    display:flex;
    flex-direction:row;
    justify-content:space-between;
    align-items: center;
    margin-right:20rpx;
    /* border-bottom: 1px solid #fff1f2 */
}
.listContent text{
    width:200rpx;
    font-size:40rpx;
    text-overflow: ellipsis;
    white-space: nowrap; 
    overflow: hidden;
}
.listIcon{
  width:44rpx;
  height:44rpx;
  /* box-shadow: 0rpx 0rpx 10rpx 0rpx #00222121; */
  margin-right:20rpx;
}
.listDetail{
  display:flex;
  flex-direction:row;
  align-items:center;
}
.listDetail text{
  font-size:32rpx;
  color:#8E8E93;
}
.sign{
    display:flex;
    flex-direction:column;
    width: 90%;
    align-items:center;
    align-self:center;
    margin:20rpx 0rpx 40rpx 0rpx;
    background-image: linear-gradient(179deg, #3023AE 0%, #E37DF5 100%);
    border-radius: 20rpx;
}
.sign-header{
  width:90%;
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  align-items:center;
  padding:10rpx 0rpx;
  margin-bottom:10rpx;
  border-bottom:1px solid rgba(255,255,255,0.5);
}
.sign-header-left{
  display:flex;
  flex-direction:row;
  align-items:center;
}
.sign-header-left image{
  margin-right:20rpx;
}
.sign-header-left text{
  font-size:14pt;
  color:white;
}
.sign-header-right{
  font-size:10pt;
  color:rgba(255,255,255,0.5);
  margin-right:40rpx;
}
.signName{
  display:flex;
  flex-direction:row;
  justify-content: space-between;
  width: 90%;
  margin-top:20rpx;
}
.signName text{
    width:42%;
    color:white;
}
.signLabel{
  display:flex;
  flex-direction:row;
}
.signLabel text{
    font-size:8pt;
    padding:0rpx 20rpx;
    text-align: center;
    border-radius:20rpx;
    color:white;
    margin:0rpx 46rpx -10rpx 54rpx;
}
.sLabel{
    background-color:#4CD964;
}
.tLabel{
    background-color:#007AFE;
}
.signTime{
  display:flex;
  flex-direction:row;
  align-items: center;
  margin-bottom: 20rpx;
}
.signTime text{
    font-size:24pt;
    text-align:center;
    color:white;
    margin:0rpx 30rpx;
}
.signTab{
  width:100%;
}
.tab-navbar{
  display:flex;
  flex-direction:row;
  justify-content: center;
  width:100%;
}
.tab-navbar-item{
  width:40%;
  text-align:center;
  font-size:12pt;
  padding:20rpx 0rpx 10rpx 0rpx;
  color:white;
}
.tab-navbar-item-on{
  background-image: linear-gradient(0deg, rgba(0,0,0,0.00) 0%, rgba(0,0,0,0.20) 100%);
  box-shadow: inset 0 1px 10px 0 rgba(0,0,0,0.02);
  border-radius: 10px 10px 0 0;
}
.btn-container{
  position:absolute;
  left:0rpx;
  bottom:100rpx;
  width:100%;
}
.submit-btn{
  width:80%;
  color:white;
  /* background-color: #0076FF; */
  background-color:#ee7f5d;
}

.modals{position:fixed; z-index: 999; top:0; left: 0; right:0; bottom: 0;}
.modals-cancel{position:absolute; z-index:1000; top:0; left: 0; right:0; bottom: 0; background-color: rgba(0,0,0,.5);}
.bottom-dialog-body{position:absolute; z-index:10001; bottom:0; left:0; right:0; padding:20rpx; height:1100rpx; background-color: #fff;}
/*动画前初始位置*/
.bottom-pos{-webkit-transform:translateY(100%);transform:translateY(100%);}
.bottom-dialog-content{
  display:flex;
  flex-direction:column;
}
.modals-scroll-view{
  display:flex;
  flex-direction:column;
  height:970rpx;
}
.modals-title{
  width:100%;
  text-align: center;
  font-size:14pt;
  font-weight:bold;
}
.modals-container{
  display:flex;
  flex-direction:column;
}
.modals-subtitle{
  margin-bottom:20rpx;
  font-size:10pt;
  font-weight:bold;
}
.modals-city-list{
  display:flex;
  flex-wrap: wrap;
  margin-bottom:20rpx;
}
.modals-alpha-list{
  display:flex;
  flex-wrap: wrap;
  margin-bottom:20rpx;
}
.modals-city-block{
  width:23%;
  padding:10rpx 0rpx;
  text-align:center;
  font-size:10pt;
  margin:0rpx 10rpx 10rpx 0rpx;
  border:1px solid #D1D1D2;
  border-radius:10rpx;
}
.modals-city-block-hover{
  color:#007AFE;
  border:1px solid #007AFE;
}
.modals-alpha-block{
  width:10.5%;
  padding:10rpx 0rpx;
  text-align:center;
  font-size:10pt;
  margin:0rpx 10rpx 10rpx 0rpx;
  border:1px solid #D1D1D2;
  border-radius:10rpx;
}
.modals-alpha-block-hover{
  color:#007AFE;
  border:1px solid #007AFE;
}
.modals-city-list-hover .modals-city-list-block-hover{
  color:#007AFE;
  border:1px solid #007AFE;
}
.search-result{
  display:flex;
  flex-direction:column;
  border-top:1px solid #F1F1D2;
}
.search-result-item{
  display:flex;
  flex-direction:row;
  align-items:center;
  padding:10rpx 20rpx;
  border-bottom:1px solid #D1D1D2;
}
.search-result-item-content{
  display:flex;
  flex-direction:column;
  padding:10rpx 0rpx;
}
.search-result-item-name{
  font-size:14pt;
}
.search-result-item-type{
  color:#808080;
  font-size:12pt;
}